{
	"name": "bankas-skafis",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"genapi": "openapi-generator-cli generate -i http://localhost/v3/api-docs -g typescript-axios -o src/services/gen-client/",
		"genapi-prod": "openapi-generator-cli generate -i https://api.bankas.skafis.lt/v3/api-docs -g typescript-axios -o src/services/gen-client/"
	},
	"devDependencies": {
		"@openapitools/openapi-generator-cli": "^2.9.0",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.7",
		"@types/node": "^20.13.0",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"flowbite": "^2.3.0",
		"flowbite-svelte": "^0.46.1",
		"flowbite-svelte-icons": "^1.6.1",
		"globals": "^15.0.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"sass": "^1.77.4",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.4.5",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "^5.0.3",
		"vitest": "^1.2.0"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-static": "^3.0.1",
		"autoprefixer": "^10.4.19",
		"axios": "^1.7.2",
		"firebase": "^10.12.2",
		"firebase-tools": "^13.10.2",
		"openapi-typescript": "^6.7.6",
		"pnpm": "^9.1.4",
		"tailwindcss": "^3.4.3"
	}
}
